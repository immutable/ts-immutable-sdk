{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "d": {
      "dependsOn": ["^d"],
      "outputs": ["{projectRoot}/dist"],
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "typecheck": {
      "cache": true,
      "dependsOn": ["build"]
    },
    "test": {
      "cache": true,
      "dependsOn": ["build"],
      "outputs": ["{projectRoot}/coverage"],
      "configurations": {
        "ci": {
          "codeCoverage": true
        }
      }
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/dist"],
      "cache": true
    },
    "export": {
      "dependsOn": [
        "build"
      ],
      "outputs": ["{projectRoot}/out"],
      "cache": true
    }
  },
  "release": {
    "projectsRelationship": "fixed",
    "projects": ["packages/**", "sdk/**", "!packages/**/*sample-app/**", "!packages/game-bridge", "!@imtbl-alpha/**"],
    "releaseTagPattern": "{version}",
    "changelog": {
        "workspaceChangelog": {
          "file": false,
          "createRelease": "github",
          "renderOptions": {
            "authors": false
          }
        },
        "git": {
          "commit": false,
          "tag": false,
          "stageChanges": false
        }
    },
    "version": {
      "generatorOptions": {
        "skipLockFileUpdate": true,
        "currentVersionResolver": "registry",
        "preid": "alpha"
      },
      "git": {
        "commit": false,
        "tag": false,
        "stageChanges": false
      }
    },
    "conventionalCommits": {
      "types": {
        "refactor": {
          "changelog": false
        },
        "style": {
          "changelog": false
        },
        "test": {
          "changelog": false
        },
        "docs": {
          "changelog": false
        },
        "build": {
          "changelog": false
        },
        "ops": {
          "changelog": false
        },
        "chore": {
          "changelog": false
        }
      }
    }
  },
  "defaultBase": "main",
  "parallel": 8,
  "nxCloudAccessToken": "Mzg3ZGY1MWUtYmYyNy00ZmE4LTkyNDAtYjYxZmJmYmE4NWQ3fHJlYWQ="
}
