{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "d": {
      "dependsOn": ["^d"],
      "outputs": ["{projectRoot}/dist"],
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "typecheck": {
      "cache": true,
      "dependsOn": ["build"]
    },
    "test": {
      "cache": true,
      "dependsOn": ["build"],
      "outputs": ["{projectRoot}/coverage"],
      "configurations": {
        "ci": {
          "codeCoverage": true
        }
      }
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/dist"],
      "cache": true
    },
    "export": {
      "dependsOn": [
        "build"
      ],
      "outputs": ["{projectRoot}/out"],
      "cache": true
    }
  },
  "release": {
    "projectsRelationship": "fixed",
    "projects": ["sdk/**", "packages/**", "!packages/**/*sample-app/**"],
    "releaseTagPattern": "{version}",
    "changelog": {
        "workspaceChangelog": {
          "file": false,
          "createRelease": false,
          "renderOptions": {
            "authors": false
          }
        }
    },
    "version": {
      "generatorOptions": {
        "skipLockFileUpdate": true,
        "currentVersionResolver": "registry",
        "preid": "alpha"
      }
    },
    "git": {
      "commit": false,
      "tag": false,
      "stageChanges": false
    },
    "conventionalCommits": {
      "types": {
        "internal": {
          "changelog": false
        }
      }
    }
  },
  "defaultBase": "main",
  "parallel": 8,
  "nxCloudAccessToken": "Mzg3ZGY1MWUtYmYyNy00ZmE4LTkyNDAtYjYxZmJmYmE4NWQ3fHJlYWQ="
}
