{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "d": {
      "dependsOn": ["^d"],
      "outputs": ["{projectRoot}/dist"],
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "typecheck": {
      "cache": true,
      "dependsOn": ["build"]
    },
    "test": {
      "cache": true,
      "dependsOn": ["build"],
      "outputs": ["{projectRoot}/coverage"],
      "configurations": {
        "ci": {
          "codeCoverage": true
        }
      }
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/dist"],
      "cache": true
    }
  },
  "defaultBase": "main",
  "parallel": 7,
  "nxCloudAccessToken": "Mzg3ZGY1MWUtYmYyNy00ZmE4LTkyNDAtYjYxZmJmYmE4NWQ3fHJlYWQ="
}
