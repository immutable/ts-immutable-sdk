{
  "name": "@imtbl/checkout-sdk",
  "description": "Provides both programatical and user interfaces for interacting with various wallets and L2 networks",
  "version": "0.0.0",
  "author": "proletesseract",
  "bugs": "https://github.com/immutable/imx-checkout-sdk/issues",
  "dependencies": {
    "@ethersproject/providers": "^5.7.2",
    "@metamask/detect-provider": "^2.0.0",
    "axios": "^1.3.5",
    "ethers": "^5.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.3",
    "babel-jest": "^29.5.0",
    "eslint": "^8.37.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "parcel": "^2.8.3",
    "typedoc": "^0.24.6",
    "typedoc-plugin-markdown": "^3.15.2",
    "typescript": "^4.9.5"
  },
  "homepage": "https://github.com/immutable/imx-checkout-sdk#readme",
  "keywords": [
    "immutablex"
  ],
  "license": "ISC",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "private": true,
  "repository": "immutable/imx-checkout-sdk.git",
  "scripts": {
    "build": "parcel build",
    "build:prod": "NODE_ENV=production parcel build --no-source-maps",
    "docs": "typedoc --plugin typedoc-plugin-markdown --skipErrorChecking --disableSources --out docs src/index.ts",
    "lint": "eslint ./src --ext .ts,.jsx,.tsx --max-warnings=0",
    "start": "parcel watch",
    "test": "jest test",
    "test:coverage": "jest --coverage",
    "test:watch": "jest test --watch",
    "typecheck": "tsc --noEmit"
  },
  "source": "src/index.ts",
  "types": "dist/types.d.ts",
  "jest": {
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    }
  }
}
