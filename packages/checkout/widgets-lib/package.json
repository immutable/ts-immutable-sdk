{
  "name": "@imtbl/checkout-widgets-lib",
  "version": "0.0.0",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@biom3/design-tokens": "0.1.5-beta",
    "@biom3/react": "^0.5.0-beta",
    "@ethersproject/providers": "^5.7.2",
    "@imtbl/checkout-sdk": "0.0.0",
    "@imtbl/checkout-widgets": "0.0.0",
    "@imtbl/config": "0.0.0",
    "@imtbl/cryptofiat": "0.0.0",
    "@imtbl/dex-sdk": "0.0.0",
    "ethers": "^5.7.2",
    "lodash.debounce": "^4.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.4.3",
    "@types/node": "^18.14.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "cypress": "^12.8.1",
    "eslint": "^8.40.0",
    "jest": "^29.4.3",
    "local-cypress": "^1.2.6",
    "react-scripts": "5.0.1",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "homepage": "https://github.com/immutable/ts-immutable-sdk",
  "keywords": [
    "immutable"
  ],
  "license": "ISC",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "private": true,
  "scripts": {
    "build": "cp src/build.tsx src/index.tsx && GENERATE_SOURCEMAP=false react-scripts build",
    "build:dist": "yarn build && yarn dist:sample",
    "dist:sample": "mkdir -p ../widgets-sample-app/public/lib/js && cp build/static/js/main.*.js ../widgets-sample-app/public/lib/js/imtbl-checkout.js",
    "dist:sdk": "mkdir -p ../../../sdk/dist/browser && cp build/static/js/main.*.js ../../../sdk/dist/browser/checkout.js",
    "eject": "react-scripts eject",
    "lint": "eslint ./src --ext .ts,.jsx,.tsx --max-warnings=0",
    "lint:fix": "eslint ./src --ext .ts,.jsx,.tsx --max-warnings=0 --fix",
    "start": "cp src/app.tsx src/index.tsx && PORT=3001 GENERATE_SOURCEMAP=false react-scripts start",
    "test": "jest test --passWithNoTests",
    "test:cypress": "NODE_ENV=test cypress run --component --headless",
    "test:cypress:open": "NODE_ENV=test cypress open --component --browser electron"
  }
}
