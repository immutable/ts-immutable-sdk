{
  "name": "@imtbl/economy",
  "description": "Economy module for Immutable SDK",
  "version": "0.0.0",
  "dependencies": {
    "@imtbl/checkout-sdk-web": "0.0.0",
    "@imtbl/checkout-ui-types": "0.0.0",
    "@imtbl/checkout-widgets": "0.0.0",
    "@swc/helpers": "~0.5.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@swc/core": "^1.3.36",
    "@swc/jest": "^0.2.24",
    "@types/node": "^18.14.2",
    "eslint": "^8.37.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "openapi-typescript": "^5.0.0",
    "prettier": "^2.8.7",
    "rollup": "^3.17.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "wsrun": "^5.2.4"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "NODE_ENV=production rollup --config rollup.config.js",
    "build:all": "wsrun -p @imtbl/economy --stages build",
    "lint": "eslint ./src --ext .ts,.jsx,.tsx --max-warnings=0",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit --jsx preserve",
    "codegen": "npx openapi-typescript https://api.games.immutable.com/inventory/swagger/doc.json --output types/inventory.d.ts && npx openapi-typescript https://api.games.immutable.com/recipe/swagger/doc.json --output types/recipe.d.ts && npx openapi-typescript https://e609qotq4d.execute-api.us-east-2.amazonaws.com/sandbox/item-definition/swagger/doc.json --output types/item-definition.d.ts && npx openapi-typescript https://e609qotq4d.execute-api.us-east-2.amazonaws.com/sandbox/crafting/swagger/doc.json --output types/crafting.d.ts"
  },
  "type": "module",
  "types": "dist/index.d.ts"
}
