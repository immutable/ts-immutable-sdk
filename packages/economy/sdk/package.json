{
  "name": "@imtbl/economy",
  "description": "Economy module for Immutable SDK",
  "version": "0.0.0",
  "dependencies": {
    "@imtbl/checkout-sdk-web": "0.0.0",
    "@imtbl/checkout-ui-types": "0.0.0",
    "@imtbl/checkout-widgets": "0.0.0",
    "@swc/helpers": "~0.5.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@swc/core": "^1.3.36",
    "@swc/jest": "^0.2.24",
    "@types/node": "^18.14.2",
    "eslint": "^8.37.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "prettier": "^2.8.7",
    "rollup": "^3.17.2",
    "swagger-typescript-api": "^12.0.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "wsrun": "^5.2.4"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "config": {
    "item_definition": "https://e609qotq4d.execute-api.us-east-2.amazonaws.com/sandbox/item-definition/swagger/doc.json",
    "crafting": "https://e609qotq4d.execute-api.us-east-2.amazonaws.com/sandbox/crafting/swagger/doc.json",
    "inventory": "https://api.dev.games.immutable.com/inventory/swagger/doc.json",
    "recipes": "https://api.dev.games.immutable.com/recipe/swagger/doc.json"
  },
  "scripts": {
    "build": "NODE_ENV=production rollup --config rollup.config.js",
    "build:all": "wsrun -p @imtbl/economy --stages build",
    "dev": "npm run build -- --watch",
    "lint": "eslint ./src --ext .ts,.jsx,.tsx --max-warnings=0",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit --jsx preserve",
    "codegen:item-definition": "rm -rf http-client/item-definition-api.ts; swagger-typescript-api -p $npm_package_config_item_definition -o ./http-client -n item-definition-api.ts",
    "codegen:crafting": "rm -rf http-client/crafting-api.ts; swagger-typescript-api -p $npm_package_config_crafting -o ./http-client -n crafting-api.ts",
    "codegen:recipes": "rm -rf http-client/recipes-api.ts; swagger-typescript-api -p $npm_package_config_recipes -o ./http-client -n recipes-api.ts",
    "codegen:inventory": "rm -rf http-client/inventory-api.ts; swagger-typescript-api -p $npm_package_config_inventory -o ./http-client -n inventory-api.ts",
    "codegen": "rm -rf http-client; npm run codegen:item-definition & npm run codegen:crafting & npm run codegen:recipes & npm run codegen:inventory"
  },
  "type": "module",
  "types": "dist/index.d.ts"
}
