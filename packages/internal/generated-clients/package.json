  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@rollup/plugin-typescript": "^11.0.0",
    "jest": "^29.4.3",
    "rollup": "^3.17.2",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=20.11.0"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/immutable/ts-immutable-sdk#readme",
  "license": "Apache-2.0",
  "main": "dist/index.js",
