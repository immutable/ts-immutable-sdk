{
  "name": "@imtbl/contracts",
  "description": "Contract bindings for internal usage",
  "version": "0.0.0",
  "author": "Immutable",
  "bugs": "https://github.com/immutable/ts-immutable-sdk/issues",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "hardhat": "^2.15.0",
    "rollup": "^3.17.2",
    "typechain": "^8.1.1"
  },
  "engines": {
    "node": ">=16.11.0"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/immutable/ts-immutable-sdk#readme",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "private": true,
  "repository": "immutable/ts-immutable-sdk.git",
  "scripts": {
    "build": "NODE_ENV=production rollup --config rollup.config.js",
    "copy:generated-abis": "node ./scripts/copy-generated-abis.js",
    "generate:abis": "hardhat compile",
    "generate:typechain-types": "typechain --target=ethers-v5 --out-dir=src/typechain/types './src/abi/*.json'",
    "generate:types": "yarn generate:abis && yarn copy:generated-abis && yarn generate:typechain-types",
    "lint": "eslint ./src --ext .ts,.jsx,.tsx --max-warnings=0",
    "prepare": "wsrun -r build",
    "typecheck": "tsc --noEmit --jsx preserve"
  },
  "type": "module",
  "types": "dist/index.d.ts"
}
