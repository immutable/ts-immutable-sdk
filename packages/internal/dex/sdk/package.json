{
  "name": "@imtbl/dex-sdk",
  "description": "DEX Provider package for the Immutable SDK",
  "version": "0.0.0",
  "author": "Immutable",
  "bugs": "https://github.com/immutable/ts-immutable-sdk/issues",
  "dependencies": {
    "@uniswap/router-sdk": "^1.4.0",
    "@uniswap/sdk-core": "^3.0.1",
    "@uniswap/v3-sdk": "^3.9.0",
    "ethers": "^5.7.2",
    "jsbi": "^3.1.4"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.0.0",
    "@swc/core": "^1.3.36",
    "@swc/jest": "^0.2.24",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/jest": "^29.4.3",
    "@types/node": "^18.14.2",
    "eslint": "^8.37.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "rollup": "^3.17.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/immutable/ts-immutable-sdk#readme",
  "keywords": [
    "immutablex"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "dist/module.js",
  "private": true,
  "repository": "immutable/ts-immutable-sdk.git",
  "scripts": {
    "build": "NODE_ENV=production rollup --config rollup.config.js",
    "generate-types": "typechain --target ethers-v5 --out-dir ./src/contracts/types ./src/contracts/ABIs/*.json",
    "lint": "eslint ./src --ext .ts --max-warnings=0",
    "lint:fix": "cd ../../../.. && yarn wsrun -p @imtbl/dex-sdk -c lint --fix",
    "test": "jest test",
    "test:watch": "jest --watch"
  },
  "source": "src/index.ts",
  "type": "module",
  "types": "dist/index.d.ts"
}
