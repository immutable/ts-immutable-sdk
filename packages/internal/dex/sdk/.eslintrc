{
  "extends": ["../../.eslintrc"],
  "ignorePatterns": ["jest.config.*", "rollup.config.*"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json",
    "tsconfigRootDir": "."
  },
  "rules": {
    "implicit-arrow-linebreak": "off",
    "operator-linebreak": "off",
    "object-curly-newline": "off",
    "no-confusing-arrow": "off"
  }
}
