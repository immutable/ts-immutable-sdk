{
  "name": "@imtbl/game-bridge",
  "version": "0.0.0",
  "dependencies": {
    "@imtbl/config": "0.0.0",
    "@imtbl/passport": "0.0.0",
    "@imtbl/version-check": "0.0.0",
    "@imtbl/x-client": "0.0.0",
    "@imtbl/x-provider": "0.0.0"
  },
  "devDependencies": {
    "parcel": "^2.8.3"
  },
  "scripts": {
    "build": "yarn build:sdk && parcel build --no-cache --no-scope-hoist && yarn updateSdkVersion",
    "build:sdk": "cd ../.. && yarn build",
    "start": "parcel",
    "updateSdkVersion": "./scripts/updateSdkVersion.sh"
  },
  "source": "src/index.html",
  "targets": {
    "unity": {
      "context": "browser",
      "source": "src/index.html",
      "engines": {
        "browsers": "Chrome 90"
      }
    },
    "unreal": {
      "outputFormat": "global",
      "context": "browser",
      "source": "src/index.ts",
      "engines": {
        "browsers": "Chrome 90"
      }
    }
  }
}
