{
  "name": "minting-sample-app",
  "version": "0.0.0",
  "_moduleAliases": {
    "@src": "dist"
  },
  "dependencies": {
    "@imtbl/sdk": "file:.yalc/@imtbl/sdk",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "ethers": "^5.7.2",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "inserturlparams": "^1.0.1",
    "jet-logger": "^1.3.1",
    "jet-paths": "^1.0.6",
    "jet-validator": "^1.1.1",
    "jsonfile": "^6.1.0",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "pg": "^8.11.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/find": "^0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jasmine": "^5.1.4",
    "@types/jsonfile": "^6.1.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.8",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.57.0",
    "eslint-plugin-node": "^11.1.0",
    "find": "^0.3.0",
    "fs-extra": "^11.2.0",
    "jasmine": "^5.1.0",
    "nodemon": "^3.1.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "src/public"
    ],
    "exec": "NODE_OPTIONS=\"--loader ts-node/esm\" ./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "scripts": {
    "build": "npx ts-node build.ts",
    "dev": "nodemon",
    "lint": "eslint ./src --ext .ts,.jsx,.tsx,.js --max-warnings=0",
    "start": "node -r module-alias/register ./dist --env=production",
    "test": "nodemon --config ./spec/nodemon.json",
    "test:no-reloading": "npx ts-node --files -r tsconfig-paths/register ./spec"
  },
  "type": "module"
}
