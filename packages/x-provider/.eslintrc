{
  "extends": ["../../.eslintrc"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json",
    "tsconfigRootDir": "."
  },
  "plugins": ["@imtbl/imtbl"],
  "rules": {
    "@imtbl/imtbl/no-namespace-export": "error",
    "@typescript-eslint/naming-convention": [
      "error",
      {
        "selector": "objectLiteralProperty",
        "format": ["camelCase", "snake_case"]
      }
    ]
  }
}
