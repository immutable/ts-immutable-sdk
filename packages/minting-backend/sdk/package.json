{
  "name": "@imtbl/minting-backend",
  "description": "minting backend utilising Immutable Minting API",
  "version": "0.0.0",
  "author": "Immutable",
  "bugs": "https://github.com/immutable/ts-immutable-sdk/issues",
  "dependencies": {
    "@imtbl/blockchain-data": "workspace:*",
    "@imtbl/config": "workspace:*",
    "@imtbl/generated-clients": "workspace:*",
    "@imtbl/metrics": "workspace:*",
    "@imtbl/webhook": "workspace:*",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@swc/core": "^1.3.36",
    "@swc/jest": "^0.2.24",
    "@testcontainers/postgresql": "^10.9.0",
    "@types/jest": "^29.4.3",
    "@types/pg": "^8.11.5",
    "@types/uuid": "^8.3.4",
    "eslint": "^8.40.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "testcontainers": "^10.9.0",
    "typescript": "^5.6.2",
    "tsup": "^8.2.4"
  },
  "exports": {
    "development": {
      "types": "./src/index.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "default": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/immutable/ts-immutable-sdk#readme",
  "license": "Apache-2.0",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "optionalDependencies": {
    "pg": "^8.11.5",
    "prisma": "^5.13.0"
  },
  "repository": "immutable/ts-immutable-sdk.git",
  "scripts": {
    "build": "pnpm transpile && pnpm typegen",
    "transpile": "tsup src/index.ts --config ./tsup.config.js",
    "typegen": "tsc --customConditions default --emitDeclarationOnly --outDir dist/types",
    "lint": "eslint ./src --ext .ts,.jsx,.tsx --max-warnings=0",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "typecheck": "tsc --customConditions default --noEmit --jsx preserve"
  },
  "type": "module",
  "types": "./dist/types/index.d.ts"
}
