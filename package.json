{
  "name": "ts-immutable-sdk",
  "version": "0.0.0",
  "description": "Immutable Multi-Rollup TypeScript SDK",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.0.0 <17.0.0"
  },
  "scripts": {
    "build": "NODE_ENV=production rollup --config rollup.config.js",
    "typecheck": "tsc --noEmit --jsx preserve",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint ./src/ --ext .ts,.jsx,.tsx --max-warnings=0",
    "lint-ci": "eslint $(git diff --name-only --diff-filter=ACMRTUXB main | grep  -E \"(.js$|.ts$|.tsx$)\") --ext .ts,.jsx,.tsx --max-warnings=0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/immutable/ts-immutable-sdk.git"
  },
  "author": "Immutable",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/immutable/ts-immutable-sdk/issues"
  },
  "homepage": "https://github.com/immutable/ts-immutable-sdk#readme",
  "dependencies": {
    "@imtbl/core-sdk": "^1.0.2-alpha.1",
    "@magic-ext/oidc": "^1.0.1",
    "@metamask/detect-provider": "^2.0.0",
    "axios": "^1.3.4",
    "ethers": "^5.7.2",
    "magic-sdk": "^13.3.1",
    "oidc-client-ts": "^2.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@swc/core": "^1.3.36",
    "@swc/jest": "^0.2.24",
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.36.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "prettier": "2.8.4",
    "rollup": "^3.17.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
