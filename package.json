{
  "name": "ts-immutable-sdk",
  "version": "0.0.0",
  "description": "Immutable Unified TypeScript SDK",
  "workspaces": [
    "packages/sdk",
    "packages/config",
    "packages/starkex",
    "packages/passport",
    "packages/provider",
    "packages/internal/toolkit"
  ],
  "private": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "wsrun --stages build",
    "test": "wsrun test",
    "lint": "eslint ./packages/ --ext .ts,.jsx,.tsx --max-warnings=0",
    "lint:path": "eslint --ext .ts,.jsx,.tsx --max-warnings=0",
    "lint:ci": "eslint $(git diff --name-only --diff-filter=ACMRTUXB $(git rev-parse HEAD) | grep  -E \"(.js$|.ts$|.tsx$)\") --ext .ts,.jsx,.tsx --max-warnings=0",
    "prepare": "husky install",
    "postinstall": "cd packages/economy && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/immutable/ts-immutable-sdk.git"
  },
  "author": "Immutable",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/immutable/ts-immutable-sdk/issues"
  },
  "homepage": "https://github.com/immutable/ts-immutable-sdk#readme",
  "devDependencies": {
    "@nrwl/eslint-plugin-nx": "^15.9.2",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "wsrun": "^5.2.4"
  }
}
